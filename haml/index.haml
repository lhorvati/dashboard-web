!!! 5
%html
  %head
    %title Codeanywhere Dashboard

    %meta(charset="utf-8")
    %meta(name="description" content="")
    %meta(name="author" content="")
    %meta(name="keywords" content="Codeanywhere, Dashboard, CA, Editor")
    %meta(id="meta" name="viewport" content="width=device-width; initial-scale=1.0, maximum-scale=1.0")

    %link(rel="stylesheet" href="css/main.css")
    %link(rel="stylesheet" href="css/balloon.css")
    %link(rel="stylesheet" href="css/query.css")
    / Fonts
    %link(rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:300,400,700&amp;subset=latin-ext")

    %script(type="text/javascript" src="js/script.js")

  %body
    //header
    #header
      .container
        .header
          .header__dashboard
            %a(href="#") Dashboard
          .header__search
            %input.input-search(placeholder="Search")
          #profile-btn.header__user
            //PROFILE POPUP
            #profile.profile
              .profile-wrap
                .profile-top.percentage-10
                  .managed-row
                  .profile-row.f-center
                    .profile-image
                      .profile-image__center
                        %span.profile-image__center--prim
                          100%
                        %span.profile-image__center--sec Profile
                        %span.profile-image__center--sec completion
                    .profile-pie-left
                    .profile-pie-right
                  .profile-row
                    .profile-details
                      .profile-details__username Luka Horvatic
                      .profile-details__email horvaticluka@gmail.com
                  .dashboard-wrap
                    .profile-row
                      %a(href="https://codeanywhere.com/dashboard" target="_blank")
                        .profile-button
                          .btn.btn--purple Dashboard
                  .upgrade-wrap
                    .profile-row
                      .upgrade-row
                        .subscription
                          Subscription:
                          %span Business Plan
                    .profile-row
                      .upgrade-row.percentage-8
                        .upgrade-row__top
                          .upgrade-row__top--bar
                        .upgrade-row__bot
                          .upgrade-row__bot--left.yellow
                            24/30
                          .upgrade-row__bot--right Containers
                    .profile-row
                      .upgrade-row.percentage-0
                        .upgrade-row__top
                          .upgrade-row__top--bar
                        .upgrade-row__bot
                          .upgrade-row__bot--left.yellow
                            37/1000
                          .upgrade-row__bot--right Remote Connections
                    .profile-row
                      .upgrade-message Need more resources?
                    .profile-row.buttons-row
                      %a(href="https://codeanywhere.com/dashboard" target="_blank")
                        .profile-button
                          .btn.btn--purple Upgrade
                      %a(href="https://codeanywhere.com/signout")
                        .txt-button
                          .btn.btn--transparent1 Sign Out




    #body
      //Side Nav
      %nav.side-nav
        %ul
          %li
            %a.side-nav-link.account.active(href="#") Account
          %li
            %a.side-nav-link.billing(href="#") Billing
          %li
            %a.side-nav-link.projects(href="#") Projects
          %li
            %a.side-nav-link.shared-with-me(href="#") Shared with me
          %li
            %a.side-nav-link.my-shares(href="#") My shares
          %li
            %a.side-nav-link.ssh-keys(href="#") SSH keys
          %li
            %a.side-nav-link.custom-stacks(href="#") Custom stacks
          %li
            %a.side-nav-link.custom-domains(href="#") Custom domains
          %li
            %a.side-nav-link.refer-friend(href="#") Refer a friend





      //MAIN SECTION - ACCOUNT ***********************************
      .main.account.show-flex
        .main__col2

          //ACCOUNT INFORMATION ================
          .card-wrap
            %h1 Account information
            .card.information
              .card__section.col

                //user
                .user
                  .user__image
                    %img(src="images/profile.jpg")
                    %a.edit-photo.hide(href="#") Edit photo
                  .user__details
                    .user__name
                      %span.title--large Mate Lopata
                    .user__email
                      %span mate.lopata@gmail.com
                    .user__company
                      %span Codeanywhere

              .card__abs-bot.row
                %button#account.btn-icon(data-balloon="Edit" data-balloon-pos="up")
                  %svg(height="100%" viewbox="0 0 24 24" width="100%" xmlns="http://www.w3.org/2000/svg")
                    %path(d="M3 17.25V21h3.75L17.81 9.94l-3.75-3.75L3 17.25zM20.71 7.04c.39-.39.39-1.02 0-1.41l-2.34-2.34c-.39-.39-1.02-.39-1.41 0l-1.83 1.83 3.75 3.75 1.83-1.83z")


              #user-details-wrap.hide
                .card__section.col
                  //user-details
                  .user-details
                    .input-wrap.first-name
                      .mui-input
                        .group
                          %input(name="" required="" type="text")/
                          %span.bar
                          %label First name
                        %span.error
                    .input-wrap.last-name
                      .mui-input
                        .group
                          %input(name="" required="" type="text")/
                          %span.bar
                          %label Last name
                        %span.error
                    .input-wrap.company
                      .mui-input
                        .group
                          %input(name="" required="" type="text")/
                          %span.bar
                          %label Company
                        %span.error

                .card__section.col.border-top
                  .email
                    %span.title--gray Update email address
                    .input-wrap.new-email
                      .mui-input
                        .group
                          %input(name="" required="" type="text")/
                          %span.bar
                          %label New email address
                        %span.error
                    .input-wrap.repeat-email
                      .mui-input
                        .group
                          %input(name="" required="" type="text")/
                          %span.bar
                          %label Please enter passowrd to confirm
                        %span.error

                .card__section
                  .card-footer
                    .user-details-close.btn.btn--transparent1 Cancel
                    %a.btn.btn--transparent1(href="#") Save


