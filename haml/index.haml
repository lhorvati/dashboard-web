      //MAIN SECTION - ACCOUNT ***********************************
      .main.account.show-flex
        .main__col2

          //ACCOUNT INFORMATION ================
          .card-wrap
            %h1 Account information
            .card.information
              .card__section.col

                //user
                .user
                  .user__image
                    %img(src="images/avatar.png")
                    %a(href="#") Edit photo
                  .user__details
                    .user__name
                      %span.title--large Mate Lopata
                    .user__email
                      %span mate.lopata@gmail.com
                    .user__company
                      %span Codeanywhere
                //user-details
                .user-details
                  .input-wrap.first-name
                    .mui-input
                      .group
                        %input(name="" required="" type="text")/
                        %span.bar
                        %label First name
                      %span.error
                  .input-wrap.last-name
                    .mui-input
                      .group
                        %input(name="" required="" type="text")/
                        %span.bar
                        %label Last name
                      %span.error
                  .input-wrap.company
                    .mui-input
                      .group
                        %input(name="" required="" type="text")/
                        %span.bar
                        %label Company
                      %span.error

              .card__section.col.border-top
